{
  "manifest_version": 3,
  "name": "Tab Grouper",
  "version": "1.12",
  "description": "Groups browser tabs by domain+common strings with unique color-coded groups. One click to (un)group. MIT License: github.com/fabceolin/tabgrouper",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    }
  },
  "permissions": ["tabs", "tabGroups"],
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
